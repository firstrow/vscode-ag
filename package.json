{
  "name": "vscode-ag",
  "displayName": "vscode-ag",
  "description": "The Silver Searcher for Visual Studio Code",
  "version": "0.0.1",
  "publisher": "zjhmale",
  "engines": {
    "vscode": "^0.10.8"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:git.viewHistory",
    "onCommand:ag.search"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "git.viewHistory",
        "title": "Git: View History (git log)"
      },
      {
        "command": "ag.search",
        "title": "AG: Search something"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/clipboard": "1.5.29",
    "@types/he": "^0.5.29",
    "@types/jquery": "^2.0.34",
    "@types/mocha": "^2.2.32",
    "@types/node": "^6.0.49",
    "@types/tmp": "0.0.31",
    "lodash": "^4.17.4",
    "mocha": "^2.3.3",
    "tslint": "^4.1.1",
    "typescript": "^2.1.5",
    "vscode": "^1.0.3"
  },
  "dependencies": {
    "@types/lodash": "^4.14.64",
    "he": "^1.1.0",
    "jquery": "^3.1.1",
    "normalize.css": "^5.0.0"
  }
}
